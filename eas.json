{
  "cli": {
    "version": ">= 9.1.0",
    appVersionSource: "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "STAGING": "production"
      },
    }
  },
  "submit": {
    "production": {}
  }
}
