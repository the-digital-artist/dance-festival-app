{
  "cli": {
    "version": ">= 9.1.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "STAGING": "production"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}