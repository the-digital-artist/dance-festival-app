{
  "expo": {
    "name": "Berlin Salsa Festival",
    "slug": "berlin-salsa-festival-fiesta-elegante",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.fiestaelegante.planner",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "instagram",
          "fb",
          "vnd.youtube"
        ],
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes during the event."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundImage": "./assets/adaptive-icon-bg.png",
        "backgroundColor": "#000000"
      },
      "package": "com.thedigitalartist.fiestaelegante.festivalapp",
      "versionCode": 1
    },
    "extra": {
      "eas": {
        "projectId": "b27cb11f-8899-42c2-89b8-4a6d767f739d"
      }
    },
    "owner": "the.digital.artist",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ]
  }
}
